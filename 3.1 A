import java.util.Scanner;

public class main{
    public static void main(String[] args){ 
        Scanner in = new Scanner(System.in);
            System.out.println();
        int x = in.nextInt();
        int result = 1;
        for (int i = 1; i <= x; i++) {
            result = result * i;
    }
        
        System.out.printf("%d \n", result);
        in.close();

    
 }
}
